{
    "name": "gobliggg/php-jenius",
    "type": "library",
    "description": "PHP Wrapper for Jenius BTPN Payment API.",
    "keywords": [
        "http",
        "rest",
        "php",
        "Jenius BTPN",
        "Jenius BTPN PHP",
        "banking",
        "HTTP client"
    ],
    "homepage": "https://github.com/gobliggg/php-jenius",
    "license": "MIT",
    "authors": [
        {
            "name": "gobliggg",
            "email": "developer.iman@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "mashape/unirest-php": "3.0.4",
        "nesbot/carbon": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0"
    },
    "autoload": {
        "psr-0": {
            "Jenius\\": "lib/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit --colors=always",
        "test:ci": "composer test -- --verbose --coverage-text --coverage-clover=coverage.xml"
    }
}
